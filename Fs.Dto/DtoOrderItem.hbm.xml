<hibernate-mapping
  xmlns="urn:nhibernate-mapping-2.2"
  namespace="FSolutions.F2x.Common.Dto"
  assembly="FSolutions.F2x.Common.Dto">

  <class name="DtoOrderItem" table="OrderItem">
    <id name="Oid" type="Guid" length="32">
      <generator class="guid.comb" />
    </id>

    <property
    name="CreatedDate"
    type="DateTime" />

    <property
      name="UpdatedDate"
      type="DateTime" />

    <property
      name="DeletedDate"
      type="DateTime" />
  </class>
</hibernate-mapping>
